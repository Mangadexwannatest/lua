                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.Parent.CFrame * CFrame.new(0,0,0)
                    fireproximityprompt(v.Parent)
                end
            end
        end
    end)
    end
    end))

