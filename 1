                                    for i,v in pairs(game.Players.LocalPlayer:GetChildren()) do 
                                        if v.Name == "PlayerGui" and v:FindFirstChild("QuestUI") == nil then
                                            getgenv().NoQuest = true
                                        elseif v:FindFirstChild("QuestUI") ~= nil then
                                            getgenv().NoQuest = false
                                        end
                                    end
                                end
                            end)
