WebhookConfig:NewButton("Test Webhook", nil, function()
    local timeInfo = os.date("*t")                                    
    BBody = game:GetService("HttpService"):JSONEncode({
          content = nil,
          embeds = {{
              ["author"] = {
                  ["name"] = "Second Piece Notify",
                  ["icon_url"] = "https://tr.rbxcdn.com/6536b9e54e326d9ebc730845842e5363/150/150/Image/Png"
              },
              ["title"] = "Second Piece", 
              ["icon_url"] = "https://tr.rbxcdn.com/6536b9e54e326d9ebc730845842e5363/150/150/Image/Png",
              ["footer"] = {
                  ["text"] = "Time : " .. timeInfo.hour .. ":" .. timeInfo.min,
                  ["icon_url"] = "https://tr.rbxcdn.com/6536b9e54e326d9ebc730845842e5363/150/150/Image/Png"
             
              },
              ["color"] = tonumber(0xFFD700),
              ["url"] = "https://www.roblox.com/games/15049111150/X2-Second-Piece",
              ["fields"] = {
              
                             {
                    ["name"] = "Test",
                    ["value"] = "Item : ".."Test",
                    ["inline"] = false
                      
                  },
                
              }
              }}
          })
      local response = syn.request({
      Url = getgenv().WebHook,
      Method = "POST",
      Headers = {
          ["Content-Type"] = "application/json"
      },
      Body = BBody
    })    
  end)

