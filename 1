                    end))

                    coroutine.resume(coroutine.create(function()
                        while wait() do pcall(function ()
                            if getgenv().AutoQuest and not getgenv().DontAcceptQuest and game.PlaceId == 15049111150 and not getgenv().STOP and not getgenv().InBossHunt and getgenv().QuestHuntOnCoolDown then
                                for i,v in pairs(game.Workspace.Quest:GetDescendants()) do
                                    if v.Name == "Quest" and not getgenv().DontAcceptQuest and not getgenv().InBossHunt and getgenv().QuestHuntOnCoolDown  then
                                        if v.Value == getgenv().Quest and not getgenv().DontAcceptQuest and not getgenv().InBossHunt and getgenv().QuestHuntOnCoolDown  then
                                            if getgenv().NoQuest and not getgenv().DontAcceptQuest and not getgenv().InBossHunt and getgenv().QuestHuntOnCoolDown  then
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.Parent.CFrame
                                            end
